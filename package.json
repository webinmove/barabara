{
  "name": "barabara",
  "version": "1.0.0",
  "description": "Automatic express-router from generic controllers",
  "main": "src/Barabara.js",
  "directories": {
    "src": "src",
    "test": "tests"
  },
  "scripts": {
    "format": "semistandard --fix",
    "test": "mocha --recursive --reporter dot",
    "test:lint": "semistandard --verbose | snazzy",
    "test:cover": "nyc --reporter=text-summary --reporter=html npm test",
    "test:coveralls": "nyc --reporter=text-lcov npm test | coveralls",
    "test:all": "npm run test:lint && npm run test:cover"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webinmove/barabara.git"
  },
  "bugs": {
    "url": "https://github.com/webinmove/barabara/issues"
  },
  "homepage": "https://github.com/webinmove/barabara#readme",
  "keywords": [
    "api",
    "automatic",
    "express",
    "json",
    "rest",
    "router",
    "routing"
  ],
  "author": "@webinmove",
  "contributors": [
    "Olivier Cuypers <oliviercuyp@gmail.com>",
    "Louis Borsu <sat@satprod.net>"
  ],
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.11",
    "to-slug-case": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "express": "^4.16.4",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "snazzy": "^8.0.0"
  },
  "engines": {
    "node": ">=8.3"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ]
  },
  "semistandard": {
    "globals": [
      "describe",
      "it",
      "before",
      "after",
      "beforeEach",
      "afterEach"
    ]
  }
}
